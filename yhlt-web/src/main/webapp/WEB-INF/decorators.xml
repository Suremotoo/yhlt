<decorators defaultdir="/decorators">
	<excludes>
		<pattern>/login</pattern>
		<pattern>/login/*</pattern>
		<pattern>/user_login.jsp</pattern>
		<pattern>/supplier_login.jsp</pattern>
	</excludes>
	<decorator name="default" page="main.jsp">
		<pattern>/*</pattern>
	</decorator>

	<decorator name="front" page="front_main.jsp">
		<pattern>/front*</pattern>
		<pattern>/front/*</pattern>
	</decorator>
</decorators>
